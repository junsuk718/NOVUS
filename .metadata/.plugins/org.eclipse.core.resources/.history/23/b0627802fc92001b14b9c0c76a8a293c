#include <as5147.h>


//constructor
AS5147::AS5147(){

}


/**
 * parity word caculate
 */
uint8_t AS5147::spiCalcEvenParity(uint16_t value){
	uint8_t cnt = 0;
	uint8_t i;

	for (i = 0; i < 16; i++)
	{
		if (value & 0x1)
		{
			cnt++;
		}
		value >>= 1;
	}
	return cnt & 0x1;
}
