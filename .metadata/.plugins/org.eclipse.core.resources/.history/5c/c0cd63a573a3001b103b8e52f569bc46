#include"spectrum.h"
#include"as5147.h"
#include"stm32g4xx_hal_uart.h"

#ifndef INC_TX_PC_H_
#define INC_TX_PC_H_


struct PAYLOAD{
	uint16_t A;
	uint16_t B;
	uint16_t C;
	uint16_t D;
	uint16_t E;
	uint16_t F;
	uint16_t G;
};

struct TrnasPacket{
	uint8_t header;
	uint8_t len;
	uint8_t msgid;
	struct PAYLOAD;
	uint8_t end;
};

void get_packet_moter();
void get_packet_rc();
void trans_pc(uint8_t len, uint8_t msgid);



struct TransPacket tp;
struct PAYLOAD payload;
#endif /* INC_TX_PC_H_ */
